<!-- This file describes category definitions available in Simal.
     Note that this is written before there is an official Simal 
     repository and therefore is intended to be the primary source
     of this data. However, once an official Simal repository
     is made available this file will be superceded by that
     repo. -->
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xmlns:doap="http://usefulinc.com/ns/doap#"
  xmlns:foaf="http://xmlns.com/foaf/0.1/"
  xmlns:dc="http://purl.org/dc/elements/1.1/"
  xmlns:asfext="http://projects.apache.org/ns/asfext#"
  xmlns:simal="http://oss-watch.ac.uk/ns/0.2/simal#">

  <doap:Project rdf:about="http://simal.oss-watch.ac.uk/categories">
    <doap:name>Simal Test Categories</doap:name>
  
    <doap:shortdesc xml:lang="en">
      Defines the test categories used by Simal.
    </doap:shortdesc>
    
    <doap:description xml:lang="en">
      A project to define the widest possible set of useful
      categories for software proejcts. These categories are
      given sensible human readable names using this project.
      
      Without this project we'd just have URI's displayed in
      the Simal system.
    </doap:description>

    <doap:mailing-list
      rdf:resource="http://groups.google.com/group/simal-contributors"
      dc:title="Users/Contributors" />
    <doap:mailing-list
      rdf:resource="http://groups.google.com/group/simal-commits"
      dc:title="Commits" />
    <doap:bug-database
      rdf:resource="http://code.google.com/p/simal/issues/list" />
    
    <doap:programming-language>RDF</doap:programming-language>
    <doap:programming-language>XML</doap:programming-language>

    <doap:maintainer>
      <foaf:Person rdf:about="http://people.apache.org/~rgardler/#me">
        <foaf:name>Ross Gardler</foaf:name>
        <rdfs:seeAlso
          rdf:resource="http://people.apache.org/~rgardler/foaf.rdf.xml" />
      </foaf:Person>
    </doap:maintainer>
    
  <!-- Simal Test Categories -->
  <doap:category rdf:resource="http://simal.oss-watch.ac.uk/category/simalIDTest#" rdfs:label="Simal ID Test" simal:categoryId="1"/>
<!-- <doap:category rdf:resource="http://simal.oss-watch.ac.uk/category/doapTest#" rdfs:label="DOAP Test"/> -->
  <doap:category rdf:resource="" rdfs:label="unknown category"/>
</doap:Project>
</rdf:RDF>
