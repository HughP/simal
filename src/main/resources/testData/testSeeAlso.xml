<!--  This is the document that defines the rdfs:seeAlso element for the main test file.
      It adds repository information to that file. -->
<Project xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xmlns:simal="http://oss-watch.ac.uk/ns/0.2/simal#"
  xmlns:foaf="http://xmlns.com/foaf/0.1/"
  xmlns:dc="http://purl.org/dc/elements/1.1/"
  xmlns="http://usefulinc.com/ns/doap#"
  rdf:about="http://foo.org/seeAlso"
  xmlns:rss="http://purl.org/rss/1.0/">
  
  <repository>
    <SVNRepository
      rdf:about="http://simal.oss-watch.ac.uk/simalTest#svnTrunk">
      <browse rdf:resource="http://svn.foo.org/viewvc/simal/trunk/" />
      <location rdf:resource="https://svn.foo.org/svnroot/simalTest" />
    </SVNRepository>
  </repository>
  
  <repository>
    <CVSRepository>
      <anon-root>:pserver:anonymous@foo.org:/cvsroot/bar</anon-root>
      <module>foobar</module>
      <browse rdf:resource="http://var.foo.org/webCVS" />
    </CVSRepository>
  </repository>
</Project>