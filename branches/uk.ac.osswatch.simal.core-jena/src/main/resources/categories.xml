  <doap:Project rdf:about="http://simal.oss-watch.ac.uk/categories">
    <doap:name>Simal Categories</doap:name>
  
    <doap:shortdesc xml:lang="en">
      Defines the categories used by Simal.
    </doap:shortdesc>
    
    <doap:description xml:lang="en">
      A project to define the widest possible set of useful
      categories for software proejcts. These categories are
      given sensible human readable names using this project.
      
      Without this project we'd just have URI's displayed in
      the Simal system.
    </doap:description>

    <doap:mailing-list
      rdf:resource="http://groups.google.com/group/simal-contributors"
      dc:title="Users/Contributors" />
    <doap:mailing-list
      rdf:resource="http://groups.google.com/group/simal-commits"
      dc:title="Commits" />
    <doap:bug-database
      rdf:resource="http://code.google.com/p/simal/issues/list" />
    
    <doap:programming-language>RDF</doap:programming-language>
    <doap:programming-language>XML</doap:programming-language>


    <doap:maintainer>
